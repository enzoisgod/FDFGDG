-- === Raccourci clavier pour afficher / cacher l‚ÄôUI ===
local UserInputService = game:GetService("UserInputService")
local panelVisible = true
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.RightShift then
        panelVisible = not panelVisible
        if panelVisible then
            Rayfield:Notify({Title = "Interface", Content = "Panneau affich√©", Duration = 3, Image = ICON_ID})
            Rayfield:Show()
        else
            Rayfield:Notify({Title = "Interface", Content = "Panneau masqu√©", Duration = 3, Image = ICON_ID})
            Rayfield:Hide()
        end
    end
end)

